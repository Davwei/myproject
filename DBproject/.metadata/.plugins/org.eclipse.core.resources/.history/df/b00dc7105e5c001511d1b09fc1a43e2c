package dosql;

import java.sql.SQLException;

import utils.Stu;

public class Insert_Join_Team {
	
	public Insert_Join_Team(String Teamid,String selfintro,Stu s){
		
		
		
		String sql="Insert into Join_Team (`Team_ID`, `Stu_ID`, `Self_Intro`)VALUSE('"+Teamid+"','"+s.getStu_ID()+"','"+selfintro+"')";
		Conn c =new Conn(sql);
		try {
			c.stmt.executeUpdate(sql);
			
			
			
			c.close();
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}

}
