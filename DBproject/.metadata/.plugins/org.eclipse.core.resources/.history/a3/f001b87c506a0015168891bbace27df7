package MainPanel;

import javax.swing.JPanel;
import javax.swing.JScrollPane;
import javax.swing.JTable;
import java.awt.BorderLayout;
import javax.swing.JButton;

public class Apply_List_tea extends JPanel {

	/**
	 * Create the panel.
	 */
	public Apply_List_tea() {
		setLayout(null);
		
		JPanel panel = new JPanel();
		panel.setBounds(10, 10, 217, 280);
		add(panel);
		panel.setLayout(new BorderLayout(0, 0));
		
		JScrollPane scrollPane = new JScrollPane();
		panel.add(scrollPane, BorderLayout.CENTER);
		
		JPanel panel_1 = new JPanel();
		panel_1.setBounds(237, 10, 203, 280);
		add(panel_1);
		panel_1.setLayout(null);
		
		JButton btnAllowIt = new JButton("Allow it");
		btnAllowIt.setBounds(24, 37, 157, 23);
		panel_1.add(btnAllowIt);
		
		JButton btnDontAllowIt = new JButton("Don't allow it");
		btnDontAllowIt.setBounds(24, 92, 157, 23);
		panel_1.add(btnDontAllowIt);

	}
}
