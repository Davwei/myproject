package dosql;

import java.sql.SQLException;

public class Delect_Team {
	public Delect_Team(String s,String team){
		Conn c;
		
		String sql ="Delect from Team where Team_ID ='"+team+"' and Stu_ID ='"+s+"'";
		c = new Conn(sql);
		try {
			c.stmt.executeUpdate(sql);
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
